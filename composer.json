{
  "name": "alecrabbit/twrb-php-components",
  "type": "library",
  "description": "PHP The WhiteRabbit Bot components",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "alecrabbit/accessories": "dev-master",
    "alecrabbit/php-helpers": "^0",
    "krowinski/bcmath-extended": "^4.1",
    "nesbot/carbon": "^2.3"
  },
  "require-dev": {
    "alecrabbit/php-output-a": "^0.0.1",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.4",
    "symfony/var-dumper": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "AlecRabbit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true
}
