{
  "name": "alecrabbit/template",
  "description": "PHP package template",
  "type": "library",
  "repositories": [
    {
      "type": "vcs",
      "url": "/var/packages/helpers"
    }
  ],
  "require": {
    "php": ">=7.2",
    "nesbot/carbon": "^2.3",
    "alecrabbit/php-helpers": "dev-master",
    "krowinski/bcmath-extended": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "nunomaduro/collision": "^2.0",
    "symfony/var-dumper": "^4.1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AlecRabbit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true
}
